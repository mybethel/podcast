{
  "name": "podcast",
  "version": 2,
  "alias": ["podcast.bethel.io"],
  "builds": [
    { "src": "index.js", "use": "@now/node" },
    { "src": "package.json", "use": "@now/static-build" }
  ],
  "routes": [
    { "src": "/(.*).xml", "dest": "/index.js" },
    { "src": "/(.*)", "dest": "/$1" }
  ],
  "github": { "silent": true }
}
